<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp"
    android:layout_marginBottom="30dp">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_id_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_id_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_id_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_id_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_transaction_reference_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/transaction_reference_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_id_title_text_view"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_transaction_reference_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/transaction_reference_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_id_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_plan_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_plan_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_transaction_reference_title_text_view"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_plan_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_plan_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_transaction_reference_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_number_of_recognised_songs_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_recognised_songs_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_plan_title_text_view"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_number_of_recognised_songs_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_recognised_songs_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_plan_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_number_of_songs_left_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_songs_left_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_number_of_recognised_songs_title_text_view"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_number_of_songs_left_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_songs_left_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_number_of_recognised_songs_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_date_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_date_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_number_of_songs_left_title_text_view"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_date_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subscription_date_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_number_of_songs_left_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_price_paid_title_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/price_paid_text"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_date_title_text_view"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/previous_subscription_price_paid_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/price_paid_text"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toBottomOf="@+id/previous_subscription_date_text_view"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>